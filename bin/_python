#! /usr/bin/env bash

_is_command_exist() {
    command -v "$1" > /dev/null
}

if _is_command_exist 'python';then
    python "$@"
elif _is_command_exist 'python2';then
    python2 "$@"
elif _is_command_exist 'python3';then
    python3 "$@"
else
    echo "cannot find python or python2 or python3"
    exit
fi
