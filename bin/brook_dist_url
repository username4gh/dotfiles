#! /usr/bin/env bash

if [[ "$#" -eq 0 ]] && [[ "$(uname -s)" == "Darwin" ]];then 
    echo "$(location https://github.com/txthinking/brook/releases/latest | sed 's/tag/download/g')/brook_macos_amd64"
fi

if [[ "$#" -eq 0 ]] && [[ "$(uname -s)" == "Linux" ]];then
    echo "$(location https://github.com/txthinking/brook/releases/latest | sed 's/tag/download/g')/brook"
fi
